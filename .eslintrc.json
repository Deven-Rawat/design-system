{
    "extends": "airbnb-base",

    "parser": "babel-eslint",

    "rules":{
      "eol-last": [2, "always"],
      "indent": [0, "space"],
      "no-multiple-empty-lines": [2, { "max": 1, "maxEOF": 0 }],
      "no-tabs": 0,
      "no-param-reassign": [
        "error", {
          "props": false
        }
      ]
    }
  }
